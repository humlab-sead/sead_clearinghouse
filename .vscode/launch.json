    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "PHP: Current File",
            "type": "php",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Attach (Remote Debug)",
            "type":"php",
            "request": "launch",
            "stopOnEntry": false,
            "localSourceRoot": "${workspaceRoot}",
            "serverSourceRoot": "${workspaceRoot}",
            "log": false,
            "port": 9000
        }
    ]
}